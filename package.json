{
  "name": "ginetta-tonic-festival",
  "homepage": "https://featival.ginetta.net/",
  "description": "Website for the Ginetta TONIC Festival",
  "version": "1.0.0",
  "author": "@mountainash",
  "private": true,
  "scripts": {
    "dev": "wrangler pages dev",
    "prepare": "husky",
    "pre-push": "yarn test",
