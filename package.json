    "test:formatting:fix": "prettier --write --ignore-unknown './**/*'"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "husky": "^9.1.1",
    "prettier": "^3.3.3",
    "wrangler": "3.66.0"
  },
  "keywords": [
    "ginetta",
    "festival",
    "cloudflare pages"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ginetta/ginetta-tonic-festival.git"
  },
  "packageManager": "yarn@4.3.1"
}
